FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    roads1
    {
        type            nutkAtmRoughWallFunction;
        z0		uniform 1.0;
	value		uniform 0;
    }
    roads3
    {
        type            nutkWallFunction;
        value           uniform 0;
    }
    roads2
    {
        type            nutkWallFunction;
        value           uniform 0;
    }
    south
    {
        type            zeroGradient;
	}
    west
    {
        type            calculated;
        value           uniform 0;
    }
    walls
    {
       type            nutkWallFunction;
        value           uniform 0;
    }
    east
    {
        type            zeroGradient;
    }
    roofs
    {
        type            nutkWallFunction;
        value           uniform 0;
    }
    north
    {
        type            zeroGradient;
    }
    atmosphere
    {
        type            symmetry;
    }
	
    #includeEtc "caseDicts/setConstraintTypes"
}


